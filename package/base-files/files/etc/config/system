
config system
	option hostname 'hg255d'
	option zonename 'Asia/Shanghai'
	option timezone 'CST-8'
	option conloglevel '8'
	option cronloglevel '8'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '1'
	list server 'ntp1.aliyun.com'
	list server 'time1.cloud.tencent.com'
	list server 'time.ustc.edu.cn'
	list server 'cn.pool.ntp.org'

config led 'led_wifi_led'
	option name 'wifi'
	option sysfs 'hg255d:wlan'
	option trigger 'netdev'
	option dev 'wlan0'
	option mode 'link tx rx'
	option default '0'

config led 'led_usb'
	option name 'USB'
	option sysfs 'hg255d:usb'
	option interval '50'
	option default '0'
	option trigger 'usbdev'
	option dev '1-1'

config led 'led_eth'
	option name 'Internet'
	option sysfs 'hg255d:internet'
	option trigger 'netdev'
	option dev 'eth0.2'
	option mode 'link tx rx'
	option default '0'

config led
	option name 'power'
	option sysfs 'hg255d:power'
	option default '1'
	option trigger 'none'

